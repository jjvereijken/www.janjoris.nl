	undef $/;
	$_=<>;
	s/(\n[ \t]*){3,}\n/\n\n\n/mg;
	s{^((   )+)}{"\t"x(length($1)/3)}mge;
	s/^\s+(?=OBJECT-TYPE)//mg;
	s/[ \t]+$//mg;
	print;

