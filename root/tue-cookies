# and we can calculate Fibonacci numbers:

sub { sub { $_[0], @_; }, sub { $_[1]
   ? ($_[0], $_[0], $_[1] - 1, $_[2] + $_[3], $_[2], @_[4..$#_])
   : ($_[4], $_[2], @_[5..$#_]);
  }, $_[0], 1, 1, @_[1..$#_];
 }, 6, \&printit, \&stopit;

# or anything else we really really want.

# It is a pity that "sub" is still needed, but apart
# from that it is all punctuation.  Most satisfactory.
