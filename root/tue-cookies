#include <stdio.h>
#include <nl_types.h>
#include <locale.h>
main()
{
	nl_catd catd;

	(void)setlocale(LC_ALL, "");
	catd = catopen("hello", 0); /* error checking omitted for brevity */
	(void)printf(catgets(catd, 1, 1, "hello, world\n"));
}
