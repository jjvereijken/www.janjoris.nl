#!/bin/perl
# sepdate, tnx to Rich Holmes, courtesy to alt.fan.warlord
require "timelocal.pl";
if ($#ARGV == 2) {
    $todmo = $ARGV[0] - 1;
    $todda = $ARGV[1];
    $todye = $ARGV[2];
    $todti = &timelocal(0,0,0,$todda,$todmo,$todye);
}
elsif ($#ARGV == -1) {
    $todti = time
    }
else {
    die 'usage: sepdate [day month year]'
    }
$septime = &timelocal(0,0,0,31,7,93);
$tdiff = $todti - $septime;
$days = int ($tdiff / (60 * 60 * 24));
($sec,$min,$hour,$mday,$mon,$year,$wday,$yday,$isdst) = localtime(time);
printf ("%3s Sep %2d %2.2d:%2.2d:%2.2d %3s 1993\n", (Sun,Mon,Tue,Wed,Thu,Fri,Sat)[$wday],$days,$hour,$min,$sec,(EST,EDT)[$isdst]);
