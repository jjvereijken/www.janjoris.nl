    s {
	[   (?# a comment by any other name)
	    '"`
	] |
	    #\s*Ceci.n'est.pas.un.comment
    } {
	foo'bar(q'life is hard in emacs')
	# oddly, this isn't *not* a comment
    }egix;  # perl's done, but emacs is hopelessly twitterpated, teehee!

