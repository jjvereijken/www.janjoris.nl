#include <topics.h>

class czar_spermit:void {
    void talk(void) {
       Subject x;
       while(1){
           set_volume(-1>>1); // hack for max int
           cout << random_select(!exists(x,SOMEONE_AGREES_WITH_X));
       } // while
    } // talk
} // class czar_spermit
