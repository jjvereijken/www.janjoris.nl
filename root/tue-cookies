#include <limits.h>

void check_size_assumptions(void)
    {
	if (CHAR_BIT*sizeof (byte) !=  8
	 || CHAR_BIT*sizeof (word) != 16
	 || CHAR_BIT*sizeof (longword) != 32
	 || ...
	) {
	    fprintf(stderr,
		"Dear user,\n"
		"   I must offer you my abject apologies.\n"
		"   This program was described as 'portable'.\n"
		"   That turns out not to be the case.\n"
		"   You are out of luck, I'm afraid.\n"
		"                   The clever wizard.\n");
#ifdef	EX_SOFTWARE
	    exit(EX_SOFTWARE);
#else
	    exit(EXIT_FAILURE);
#endif
    }
